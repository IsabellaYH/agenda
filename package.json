{
  "name": "agendapro",
  "version": "1.0.0",
  "scripts": {
    "build:css": "sass css/scss/main.scss css/style.css --no-source-map --style=expanded",
    "build:css:prod": "sass css/scss/main.scss css/style.css --no-source-map --style=compressed",
